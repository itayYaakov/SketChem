        "@types/object-hash": "^2.2.1",
        "@types/rbush": "^3.0.0",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "bootstrap": "^5.1.3",
        "clsx": "^1.1.1",
        "hotkeys-js": "^3.9.4",
        "lodash": "^4.17.21",
        "object-hash": "^3.0.0",
        "path-browserify": "^1.0.1",
        "rbush": "^3.0.1",
        "react": "^17.0.2",
        "react-bootstrap": "^2.2.3",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-scripts": "5.0.0",
        "redux": "^4.1.2",
        "redux-undo": "^1.0.1",
        "tinyqueue": "^2.0.3",
        "typescript": "^4.6.2"
    },
    "scripts": {
        "start": "craco start",
        "pretty": "yarn run lint_fix && yarn run prettier:write",
        "build": "yarn run lint_fix && yarn run prettier:write && craco build",
        "test": "craco test",
        "tsc": "tsc",
        "debugchrome": "cross-env BROWSER='firefox' BROWSER_ARGS='--remote-debugging-port=9222' craco start",
        "eject": "craco eject",
        "test:prettier": "prettier --check \"./**/*.{js,ts,jsx,tsx,json}\"",
        "test:prettier_verbose": "prettier -l \"./**/*.{js,ts,jsx,tsx,json}\"",
        "prettier:write": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint_fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-react": "^7.16.7",
        "@types/lodash": "^4.14.182",
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.13.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
